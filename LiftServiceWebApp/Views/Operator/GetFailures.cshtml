@{ ViewBag.Title = "Arızalar";

}
@model List<AssignedFailureViewModel>
<div class="container-fluid ss_service_one ss_section_sec_bg spacer_top spacer_bottom ss_two">
    <div class="row">
        <div class="container">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <br />
                <br />
                <br />
                <div class="container">
                    <br>
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" data-toggle="tab" href="#menu1">Gelen Arızalar</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#menu2">Alınan Arızalar</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#menu3">Beklemede</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#menu4">Ödeme Bekleyenler</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#menu5">Tamamlanmış Arızalar</a>
                        </li>
                    </ul>

                    <!--Tab1-->
                    <div class="tab-content">

                        <div id="menu1" class="container tab-pane active">
                            <br>
                            <h3>Gelen Arızalar</h3>
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Arıza</th>
                                        <th>Arıza Açıklaması</th>
                                        <th>Adres</th>
                                        <th>Oluşturulma Tarihi</th>
                                        <th>Teknisyen</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 1; i <= Model.Count(); i++)
                                    {
                                        @if (Model[i - 1].FailureState == FailureStates.Alındı)
                                        {
                                            <tr>
                                                <th scope="row">@i</th>
                                                <td>@Model[i - 1].FailureName</td>
                                                <td>@Model[i - 1].FailureDescription</td>
                                                <td>@Model[i - 1].AddressDetail</td>
                                                <td>@Model[i - 1].CreatedDate</td>
                                                <td>@Model[i - 1].TechnicianName</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                        <!--Tab2-->
                        <div id="menu2" class="container tab-pane fade">
                            <br>
                            <h3>Alınan Arızalar</h3>
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Arıza Adı</th>
                                        <th>Arıza Açıklama</th>
                                        <th>Adres</th>
                                        <th>Oluşturulma Tarihi</th>
                                        <th>Teknisyen Adı</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 1; i <= Model.Count(); i++)
                                    {
                                        @if (Model[i - 1].FailureState == FailureStates.Yonlendirildi)
                                        {
                                            <tr>
                                                <th scope="row">@i</th>
                                                <td>@Model[i - 1].FailureName</td>
                                                <td>@Model[i - 1].FailureDescription</td>
                                                <td>@Model[i - 1].AddressDetail</td>
                                                <td>@Model[i - 1].CreatedDate</td>
                                                <td>@Model[i - 1].TechnicianName</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                        <!--Tab3-->
                        <div id="menu3" class="container tab-pane fade">
                            <br>
                            <h3>Beklemede</h3>
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Arıza Adı</th>
                                        <th>Arıza Açıklama</th>
                                        <th>Adres</th>
                                        <th>Oluşturulma Tarihi</th>
                                        <th>Teknisyen Adı</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 1; i <= Model.Count(); i++)
                                    {
                                        @if (Model[i - 1].FailureState == FailureStates.KabulEdildi)
                                        {
                                            <tr>
                                                <th scope="row">@i</th>
                                                <td>@Model[i - 1].FailureName</td>
                                                <td>@Model[i - 1].FailureDescription</td>
                                                <td>@Model[i - 1].AddressDetail</td>
                                                <td>@Model[i - 1].CreatedDate</td>
                                                <td>@Model[i - 1].TechnicianName</td>
                                            </tr>
                                        }
                                    }

                                </tbody>
                            </table>
                        </div>
                        <!--Tab4-->
                        <div id="menu4" class="container tab-pane fade">
                            <br>
                            <h3>Ödeme Bekleyenler</h3>
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Arıza Adı</th>
                                        <th>Arıza Açıklama</th>
                                        <th>Adres</th>
                                        <th>Oluşturulma Tarihi</th>
                                        <th>Teknisyen Adı</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                        <!--Tab5-->
                        <div id="menu5" class="container tab-pane fade">
                            <br>
                            <h3>Tamamlanmış Arızalar</h3>
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Arıza Adı</th>
                                        <th>Arıza Açıklama</th>
                                        <th>Adres</th>
                                        <th>Oluşturulma Tarihi</th>
                                        <th>Teknisyen Adı</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 1; i <= Model.Count(); i++)
                                    {
                                        @if (Model[i - 1].FailureState == FailureStates.OdemeBekleniyor)
                                        {
                                            <tr>
                                                <th scope="row">@i</th>
                                                <td>@Model[i - 1].FailureName</td>
                                                <td>@Model[i - 1].FailureDescription</td>
                                                <td>@Model[i - 1].AddressDetail</td>
                                                <td>@Model[i - 1].CreatedDate</td>
                                                <td>@Model[i - 1].TechnicianName</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                        <!--Tab5-->
                        <div id="menu5" class="container tab-pane fade">
                            <br>
                            <h3>Tamamlanmış Arızalar</h3>
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Arıza Adı</th>
                                        <th>Arıza Açıklama</th>
                                        <th>Adres</th>
                                        <th>Oluşturulma Tarihi</th>
                                        <th>Teknisyen Adı</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 1; i <= Model.Count(); i++)
                                    {
                                        @if (Model[i - 1].FailureState == FailureStates.Sonuclandi)
                                        {
                                            <tr>
                                                <th scope="row">@i</th>
                                                <td>@Model[i - 1].FailureName</td>
                                                <td>@Model[i - 1].FailureDescription</td>
                                                <td>@Model[i - 1].AddressDetail</td>
                                                <td>@Model[i - 1].CreatedDate</td>
                                                <td>@Model[i - 1].TechnicianName</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div>
            <br />
        </div>
    </div>
</div>
